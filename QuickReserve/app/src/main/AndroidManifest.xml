<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quickreserve.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/quickreserve_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".DateTimeActivity"
            android:label="@string/title_activity_selectDateTime"
            android:parentActivityName=".LoginActivity" >
        </activity>
        <activity
            android:name=".MyReservationActivity"
            android:label="@string/title_activity_my_reservation" >
        </activity>
        <activity
            android:name=".ViewReservationActivity"
            android:label="@string/title_activity_view_reservation" >
        </activity>
        <activity
            android:name=".EditReservationTimeActivity"
            android:label="@string/title_activity_edit_reservation_time" >
        </activity>
        <activity
            android:name=".EditReservationSeatActivity"
            android:label="@string/title_activity_edit_reservation_seat" >
        </activity>
        <activity
            android:name=".EditReservationActivity"
            android:label="@string/title_activity_edit_reservation" >
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name=".testActivity"
            android:label="@string/title_activity_test" >

        </activity>
    </application>

</manifest>
