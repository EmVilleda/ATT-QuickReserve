<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quickreserve.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="quickreserve.app.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="quickreserve.app.MyReservationActivity"
            android:label="@string/title_activity_my_reservation"
            android:parentActivityName=".MapActivity">
        </activity>
        <activity
            android:name="quickreserve.app.ViewReservationActivity"
            android:label="@string/title_activity_view_reservation" >
        </activity>
        <activity
            android:name="quickreserve.app.EditReservationTimeActivity"
            android:label="@string/title_activity_edit_reservation_time" >
        </activity>
        <activity
            android:name="quickreserve.app.EditReservationSeatActivity"
            android:label="@string/title_activity_edit_reservation_seat" >
        </activity>
        <activity
            android:name="quickreserve.app.EditReservationActivity"
            android:label="@string/title_activity_edit_reservation" >
        </activity>
        <activity
            android:name="quickreserve.app.MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
    </application>

</manifest>
